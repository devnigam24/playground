{
    "files.exclude": {
      "**/*.DS_Store": true,
      ".vscode/": true,
      "**/*.js": true,
      "**/.git": false,
      "node_modules/": true,
      "package-lock.json": true,
      "tsconfig.json": true,
      "yarn.lock": true
    },
    "prettier.printWidth": 100,
    "prettier.tabWidth": 2,
    "prettier.singleQuote": true,
    "prettier.parser": "flow",
    "eslint.enable": true,
    "html.format.enable": false,
    "editor.tabSize": 2
  }