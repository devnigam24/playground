{
    "restartable": "rs",
    "ignore": [
      "**/.git",
      ".gitignore",
      "**/node_modules",
      "node_modules/**",
      "**/.sass-cache",
      "**/bower_components",
      "LICENSE",
      "README.json",
      "yarn.lock"
    ],
    "verbose": false,
    "watchOptions": {},
    "execMap": {
      "js": "tsc -p . && node server.js"
    },
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "ts,html,json"
  }